{
  "version": "0.1.0",
  "backend": "CUDA",
  "data": {
    "source": {
      "type": "csv",
      "path": "/data/training.csv",
      "has_header": true,
      "delimiter": ","
    },
    "scheme": {
      "columns": [
        {
          "name": "feature1",
          "type": "float"
        },
        {
          "name": "feature2",
          "type": "float"
        },
        {
          "name": "category",
          "type": "string"
        },
        {
          "name": "label",
          "type": "float"
        }
      ],
      "target_column": "label"
    }
  },
  "transformation_pipeline": [
    {
      "name": "categorical_encoding",
      "operation": "one_hot_encode",
      "params": {
        "columns": [
          "category"
        ]
      }
    },
    {
      "name": "normalization",
      "operation": "min_max_scale",
      "params": {
        "columns": [
          "feature1",
          "feature2"
        ],
        "range": [
          0,
          1
        ]
      }
    }
  ],
  "models": [
    {
      "id": "m1",
      "model_type": "NeuralNetwork",
      "input_columns": [
        "feature1",
        "feature2",
        "category_encoded"
      ],
      "hyperparams": {
        "layers": [
          64,
          32,
          16
        ],
        "activation": "relu",
        "optimizer": "adam",
        "epochs": 10
      },
      "output": {
        "type": "embedding",
        "shape": [
          16
        ]
      }
    },
    {
      "id": "m2",
      "model_type": "DeepQNetwork",
      "input_from": "m1",
      "hyperparams": {
        "layers": [
          32,
          16
        ],
        "activation": "relu",
        "optimizer": "rmsprop",
        "exploration_rate": 0.1
      },
      "output": {
        "type": "policy_vector",
        "shape": [
          4
        ]
      }
    },
    {
      "id": "m3",
      "model_type": "NeuralNetwork",
      "input_from": "m2",
      "hyperparams": {
        "layers": [
          16,
          8
        ],
        "activation": "relu",
        "optimizer": "adam",
        "epochs": 5
      },
      "output": {
        "type": "prediction",
        "output_mode": "classification"
      }
    },
    {
      "id": "m4",
      "model_type": "GradientBoostedDecisionTree",
      "input_columns": [
        "feature1",
        "feature2",
        "category_encoded"
      ],
      "hyperparams": {
        "n_trees": 100,
        "learning_rate": 0.1,
        "max_depth": 5
      },
      "output": {
        "type": "prediction",
        "output_mode": "classification"
      }
    },
    {
      "id": "m5",
      "model_type": "Ensemble",
      "method": "voting",
      "members": [
        {
          "id": "m3",
          "weight": 1.0
        },
        {
          "id": "m4",
          "weight": 2.0
        }
      ],
      "output_mode": "probabilities"
    }
  ],
  "output": {
    "final_output": "m5",
    "include_probabilities": true
  }
}